{
  "manifest_version": 3,
  "name":"name",
  "description": "desc",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "permissions": ["tabs","scripting","activeTab","cookies"],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": {
    "extension_pages": "style-src 'self' https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css; object-src;script-src 'self'; default-src ; script-src-elem 'self'"
  }
}
